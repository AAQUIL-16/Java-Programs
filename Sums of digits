import java.util.*;
public class sum_of_the_digits{
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        System.out.print("Enter the number:");
        int n=s.nextInt();
        int sum=0;
        int rem;
        while(n>0){
            rem=n%10;
            sum=sum+rem;
            n=n/10;
        }
        System.out.println("Sum of the digits:"+sum);
    }
}
