import java.util.*;
public class Reverse{
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        System.out.print("Enter the number:");
        int a=s.nextInt();
        int ans=0;
        int rem;
        while(a>0){
            rem=a%10;
            ans=ans*10+rem;
            a=a/10;
        }
        System.out.println("Reverse number: "+ ans);
    }
}
